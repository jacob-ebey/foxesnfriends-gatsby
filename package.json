{
  "name": "foxesnfriends",
  "description": "Foxes n Friends website.",
  "version": "1.0.0",
  "author": "Jacob Ebey",
  "license": "MIT",
  "main": "n/a",
  "keywords": [
    "gatsby",
    "lambda",
    "foxes",
    "n",
    "friends"
  ],
  "scripts": {
    "clean": "gatsby clean && rimraf lambda-build",
    "ci": "npm run lint && npm run build",
    "build": "npm run clean && npm run build:lambda && npm run build:frontend",
    "build:frontend": "gatsby build",
    "build:lambda": "netlify-lambda build functions",
    "dev": "npm run clean && run-p dev:*",
    "dev:frontend": "dotenv gatsby develop",
    "dev:lambda": "nodemon",
    "lint": "eslint gatsby-*.js functions/**/*.js src/**/*.js src/**/*.jsx ",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "exec": "dotenv netlify-lambda serve functions",
    "watch": [
      "functions/"
    ]
  },
  "devDependencies": {
    "dotenv-cli": "^3.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gatsby-plugin-env-variables": "^1.0.1",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-netlify": "^2.0.6",
    "gatsby-plugin-netlify-cms": "^4.1.37",
    "gatsby-plugin-purgecss": "^4.0.0",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.0.7",
    "gatsby-plugin-sharp": "^2.2.9",
    "gatsby-remark-copy-linked-files": "^2.0.7",
    "gatsby-remark-images": "^3.1.6",
    "gatsby-remark-relative-images": "^0.2.1",
    "gatsby-source-filesystem": "^2.0.26",
    "gatsby-transformer-remark": "^2.6.9",
    "gatsby-transformer-sharp": "^2.1.9",
    "http-proxy-middleware": "^0.20.0",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.4",
    "lynt": "^0.5.5",
    "marked": "^0.8.0",
    "netlify-lambda": "^1.6.3",
    "node-object-hash": "^2.0.0",
    "node-sass": "^4.11.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "urlify": "^0.3.6",
    "uuid": "^3.2.1",
    "yahoo-stock-prices": "^1.0.1"
  },
  "dependencies": {
    "@sentry/browser": "^5.11.2",
    "apollo-server": "^2.9.16",
    "apollo-server-lambda": "^2.14.2",
    "front-matter": "^3.1.0",
    "gatsby": "^2.13.31",
    "gatsby-image": "^2.0.23",
    "logrocket": "^1.0.6",
    "marked": "^0.8.0",
    "moment": "^2.24.0",
    "netlify-cms-app": "^2.11.9",
    "netlify-cms-media-library-cloudinary": "^1.3.4",
    "netlify-cms-media-library-uploadcare": "^0.5.5",
    "newsapi": "^2.4.0",
    "newsapi-netlify-lambda": "^2.4.0",
    "node-fetch": "^2.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "react-sharingbuttons": "^1.0.0",
    "reset-css": "^5.0.1"
  }
